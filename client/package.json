  "scripts": {
    "ng": "ng",
    "start:server": "dotnet run -p ../server",
    "start:client": "ng serve",
    "start": "concurrently --raw --kill-others \"yarn start:server\" \"yarn start:client\"",
    "build:server": "dotnet build ../server",
    "build:client": "ng build",
    "build": "yarn build:server & yarn build:client",
